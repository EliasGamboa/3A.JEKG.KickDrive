@page
@model KickDrive.Pages.RegistroSupervisionModel
@{
    ViewData["Title"] = "RegistroSupervision";
}

<h1>Registro Supervisión</h1>


<body>
    <div>
        <label>Llene el siguiente formulario</label>
    </div>

    <div>
        <form class="mt-3" method="post">
            <div style="float:right"><a class="nav-link text-dark" asp-area="" asp-page="/index">Atras</a></div>
            <div>

                <div class="row form-group">
                    <label></label>
                </div>
                <div class="row form-group">
                    <label class="col-form-label col-sm-2" asp-for="ProcesoRevision.HorarioSupervision">Ingrese el horario de la supervision</label>
                    <input type="text" class="form-control form-text" asp-for="ProcesoRevision.HorarioSupervision" role="textbox" aria-label="Small" aria-describedb HorarioSupervision="HorarioSupervision" id="HorarioSupervision" placeholder="Horario de Supervision" />
                </div>
                <div class="row form-group">
                    <label class="col-form-label col-sm-2" asp-for="ProcesoRevision.Incidentes">Ingrese si hubo incidentes</label>
                    <input type="text" class="form-control form-text" asp-for="ProcesoRevision.Incidentes" role="textbox" aria-label="Small" aria-describedb Incidentes="Incidentes" id="Incidentes" placeholder="Ingrese si hubo incidentes" />
                </div>
                <div class="row form-group">
                    <label class="col-form-label col-sm-2" asp-for="ProcesoRevision.Ruta">Ingrese la ruta</label>>
                    <input type="text" class="form-control form-text" asp-for="ProcesoRevision.Ruta" role="textbox" aria-label="Small" aria-describedb Ruta="Ruta" id="Ruta" placeholder="Ingrese la ruta" />
                </div>
                <div class="row form-group">
                    <label class="col-form-label col-sm-2" asp-for="ProcesoRevision.Supervisor">Ingrese el nombre del supervisor</label>
                    <input type="text" class="form-control form-text" asp-for="ProcesoRevision.Supervisor" role="textbox" aria-label="Small" aria-describedb Supervisor="Supervisor" id="Supervisor" placeholder="Ingrese el nombre del supervisor" />
                </div>
                <div class="row form-group">
                    <label class="col-form-label col-sm-2" asp-for="ProcesoRevision.InfoViajes">Ingrese la informacion del viaje</label>
                    <input type="text" class="form-control form-text" asp-for="ProcesoRevision.InfoViajes" role="textbox" aria-label="Small" aria-describedb InfoViajes="InfoViajes" id="InfoViajes" placeholder="Ingrese la informacion del viaje" />
                </div>
            </div>
            <div class="d-flex justify-content-end p-2">
                <button class="btn btn-primary m-2" role="button" type="submit">Registrar</button>
            </div>
            <div class="text-danger" asp-validation-summary="All"></div>
        </form>
        </div>

</body>